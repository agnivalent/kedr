{{#offices}}
<div>
	<div onclick="javascript:selectOffice(this)" class="location inline">{{name}}</div><br/>
	<div class="office-info inline display-none">
		<hr/>
		<p class="address">{{address}}</p>
		<p class="phone">{{phone}}</p>
		{{#operation-time}}
			<p class="operation-time">{{.}}</p>
		{{/operation-time}}
	</div>
</div>

{{/offices}}