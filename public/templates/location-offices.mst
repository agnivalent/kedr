{{#offices}}
<div>
	<div onclick="javascript:selectOffice(this)" class="location inline">{{name}}</div><br/>
	<div class="office-info inline display-none">
		<hr/>
		<p>{{address}}</p>
		<p>{{phone}}</p>
		{{#operation-time}}
			<p>{{.}}</p>
		{{/operation-time}}
		<hr/>
	</div>
</div>

{{/offices}}